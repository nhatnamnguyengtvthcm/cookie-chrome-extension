{
    "manifest_version": 3,
    "name": "Cookie Reader",
    "version": "1.0",
    "permissions": [
        "tabs",
        "cookies",
        "activeTab",
        "storage",
        "declarativeContent",
        "alarms"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [ "*://*/*" ]
}